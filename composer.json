{
    "name": "termyn/cqrs",
    "description": "CQRS architecture based on the Symfony Messenger 4.3 and higher",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edo Barnas",
            "email": "iam@edobarnas.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Termyn\\Cqrs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Termyn\\Cqrs\\Test\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/messenger": "6.2.*",
        "symfony/validator": "6.2.*",
        "termyn/identifier": "dev-main",
        "termyn/timekeeper": "0.3.*"
    },
    "require-dev": {
        "phpstan/phpstan": "1.9.*",
        "phpstan/phpstan-phpunit": "1.3.*",
        "phpunit/phpunit": "9.5.*",
        "symplify/easy-coding-standard": "11.1.*"
    }
}
